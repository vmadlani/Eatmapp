<p id="notice"><%= notice %></p>

<h1>Listing Outlet2s</h1>


<%= form_tag outlet2s_path, :method => :get do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search Near", :name => nil %>
  </p>
<% end %>



<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outlet2s.each do |outlet2| %>
      <tr>
        <td><%= outlet2.address %></td>
        <td><%= outlet2.latitude %></td>
        <td><%= outlet2.longitude %></td>
        <td><%= link_to 'Show', outlet2 %></td>
        <td><%= link_to 'Edit', edit_outlet2_path(outlet2) %></td>
        <td><%= link_to 'Destroy', outlet2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outlet2', new_outlet2_path %>
